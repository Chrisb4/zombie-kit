<% include ./partials/header %>

<div class="container-fluid">
  <div class="row">
  <div class="col-md-3"></div>
  <div class="col-md-6 cart-test">


  <div id="cart-form">
    <form method="GET" action="http://www.amazon.com/gp/aws/cart/add.html" target="_blank">
      <input type="hidden" name="AWSAccessKeyId" value="Access Key ID" />
      <input type="hidden" name="AssociateTag" value="Associate Tag" />

      <!-- <div id="cart-list"> -->
        <ul id="ul-display">
        <% for(var i = 0; i < cartItems.length; i++) { %>
           <li id="items<%=i%>">
           <%= cartItems[i].title %>
           <%= cartItems[i].price %>
           <input type="hidden" name="ASIN.<%= i + 1 %>" value="<%= cartItems[i].ASIN %>"/>
           <input type="hidden" name="Quantity.<%= i + 1 %>" value="1"/>
           </li>
        <% } %>
        </ul>


      <input type="submit" name="add" value="Buy this on Amazon" class="btn" id="sub-btn" />
    </form>
     </div>
    </div>
    <div class="col-md-3"></div>
  </div>




  </div>





<% include ./partials/footer %>
